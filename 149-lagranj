#include <iostream>
#include <cmath>

//главная функция
int main ()
{
  int a;
  std::cin >> a;
  
  for (int i=1;i<=sqrt(a);i++)
    {
      for (int j=1;j<=sqrt(a);j++)
        {
          for (int k=1;k<=sqrt(a);k++)
            {
              for (int l=1;l<=sqrt(a);l++)
                {
                  if (i*i+j*j+k*k+l*l == a)
                    std::cout << i << " " << j << " " << k << " " << l << std::endl;
                    break;
                }
            }
        }
    }
  return 0;
}
