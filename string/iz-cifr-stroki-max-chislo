
#include <iostream>
#include <vector>
#include <string>
#include <stdio.h>
using namespace std;

int main()
{
    string a, a1,a2;
    getline(cin, a);
    for (int i=0;i<a.size();i++)
      {
        if (a[i]>='0' and a[i]<='9')
        {
          a1+=a[i];
        }
      }
    for (int j='9';j>='0';j--)
      {
      for (int i=0;i<a1.size();i++)
        { 
          if (a1[i]==j)
         {
           a2+=a1[i];
         }
      }
    }   
    cout <<a2;
    return 0;
}
