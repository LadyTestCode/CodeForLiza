// Online C++ compiler to run C++ program online
#include <iostream>

int main() {
    int a; //число элементов массива
    std::cin >> a;
    int ar[a];
//заполнили массив
    for (int i=0; i<a; i++)
    {
        std::cin >> ar[i] ;
    }
    int c; //число для поиска
    std::cin >> c;
    int ardif[a];

    int t = 0;//счетчик

    for(int i = 0; i < a; i++)
    {
        ardif[i]=ar[i]-c;
    }
    int min=ardif[i];
    int j=0;
    for(int i = 0; i < a; i++)
    {
        if (ardif[i]<min)
        {min = ardif[i];
         j=i;
        }
      
    }
    std::cout << ar[j];
    return 0;
}
