// Online C++ compiler to run C++ program online
#include <iostream>

  int main() {
    int n,k,s; 
    int ar[150000];

    std::cin >>n>>k;
    
  //заполнили массив
    for (int j=0; j<n; j++)
    {
      std::cin >> ar[j] ;
    }
  
    for(int i = 0; i < n-k+1; i++)
    {
      int min = ar[i];
      s=i;
      for (int j=i;j< i+k; j++)
      {
        if(ar[j] < min)
        {
          min = ar[j];
          s=j;//запоминаем позицию минимального элемента
        }
      }
      std::cout << ar[s] << std::endl;
    }
    return 0;
}
