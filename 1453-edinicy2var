
#include <iostream>
//вариант от лицеистов
using namespace std;

int main() {
    int N, r = 0;
    cin >> N;
    for (int n = 0; n <= N; ++n) {
        r = (r * 10 + 1) % N;
        if (r == 0) {
            for (int i = 0; i <= n; ++i) cout << 1;
            cout << endl;
            return 0;
        }
    }
    cout << "NO\n";
  return 0;
}
