// Online C++ compiler to run C++ program online
#include <iostream>

int main() {
    int a; //первое число
    int b; //второе число
    std::cin >> a >> b;
    int n; //число чисел в массиве
    n = b-a+1; //вычисляем, сколько чисел в массиве
    int arraytest[n]; //инициируем массив целых чисел
   
    for(int i = a; i <=b; i++) //начинаем цикл от одного числа до второго с шагом 1
    {
     arraytest[i] = i*i;//в задаче элемент массива равен квадрату индекса
     std::cout << arraytest[i]<< " "; //в этом же цикле можно сразу массив и печатать
    }
    return 0;
}

---------------------
//Второй вариант, который точно будет работать для всех чисел
// Online C++ compiler to run C++ program online
#include <iostream>

int main() {
    int a; //первое число
    int b; //второе число
    std::cin >> a >> b;
    int n; //число чисел в массиве
    n = b-a+1; //вычисляем, сколько чисел в массиве
    int arraytest[n]; //инициируем массив целых чисел, индексы буду использовать от 0 как надо в массиве, но значения вставляю как квадрат нужных заданных чисел
    int i=0;
    for (int j = a; j = b; j++){
      arraytest[i] = j*j;//в задаче элемент массива равен квадрату индекса
      std::cout << arraytest[i]<< " "; //в этом же цикле можно сразу массив и печатать
      i++  ; 
   }
 return 0;
}
